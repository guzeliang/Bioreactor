<Window x:Class="Shunxi.App.CellMachine.Controls.ManualSet"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Shunxi.App.CellMachine.Controls"
        mc:Ignorable="d"
        Title="ManualSet" Height="600" Width="800">
    <StackPanel>
        <TextBlock x:Name="txtRet" FontSize="30" Margin="10" Foreground="Red"></TextBlock>
        <StackPanel Orientation="Horizontal">
            <Button Content="快速充气" Width="140" Height="40" x:Name="btnFastFillGas" Click="BtnFastFilling_OnClick"></Button>
            <Button Content="停止充气" Width="140" Height="40" x:Name="btnStopFillGas" Click="BtnStopFillGas_OnClick" Margin="10"></Button>
            <TextBlock x:Name="txtFillGas" VerticalAlignment="Center"></TextBlock>
        </StackPanel>
    </StackPanel>
</Window>
